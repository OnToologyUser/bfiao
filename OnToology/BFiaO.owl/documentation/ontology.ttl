                                                  rdfs:range <https://w3id.org/def/BFiaO/BFiaO#node> ;
                                                  rdfs:comment "It defines the status kept by a node. It s the inverse object property of hasStatus."@en ;
                                                  rdfs:isDefinedBy "It defines the status kept by a node. It s the inverse object property of hasStatus."@en ;
                                                  rdfs:label "isStatusKeptBy"@en .


